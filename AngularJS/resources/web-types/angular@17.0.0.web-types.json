            ]
          }
        },
        {
          "name": "Banana box binding",
          "virtual": true,
          "value": {
            "required": true,
            "type": "of-match",
            "kind": "expression"
          },
          "ng-binding-pattern": true,
          "pattern": {
            "items": "/js/ng-directive-in-outs",
            "or": [
              [
                "[(",
                "#item",
                ")]"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "bindon-"
                  ]
                },
                "#...",
                "#item"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "data-"
                  ]
                },
                "#...",
                "bindon-",
                "#...",
                "#item"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "data-"
                  ]
                },
                "#...",
                "[(",
                "#item",
                ")]"
              ]
            ]
          },
          "js": {
            "ng-directive-in-outs": [
              {
                "name": "Unknown input/output",
                "priority": "lowest",
                "pattern": ".+",
                "doc-hide-pattern": true,
                "ng-error-symbol": true
              }
            ]
          }
        },
        {
          "name": "Template binding",
          "virtual": true,
          "value": {
            "required": true,
            "kind": "expression"
          },
          "ng-binding-pattern": true,
          "pattern": {
            "items": "/js/ng-structural-directives",
            "or": [
              [
                "*",
                "#item:Structural Directive"
