          },
          "value": {
            "kind": "plain",
            "type": "string",
            "required": false
          },
          "ng-i18n-attributes": [
            {
              "name": "Absent attribute name",
              "priority": "lowest",
              "proximity": 0,
              "pattern": ".+",
              "doc-hide-pattern": true,
              "ng-error-symbol": true,
              "hide-from-completion": true
            }
          ]
        }
      ],
      "ng-property-bindings": [
