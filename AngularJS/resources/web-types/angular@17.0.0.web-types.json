              [
                {
                  "priority": "lowest",
                  "template": [
                    "data-"
                  ]
                },
                "#...",
                "*",
                "#item:Structural Directive"
              ]
            ]
          },
          "js": {
            "ng-structural-directives": [
              {
                "name": "Unknown structural directive",
