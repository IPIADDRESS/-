            "kind": "expression"
          },
          "ng-binding-pattern": true,
          "pattern": {
            "items": "/css/classes",
            "template": [
              {
                "priority": "lowest",
                "template": [
                  "class."
                ]
              },
              "#...",
              "#item:CSS Class"
