          "value": {
            "kind": "expression",
            "type": "void"
          },
          "ng-binding-pattern": true,
          "pattern": {
            "delegate": "ng-event-binding-items",
            "or": [
              [
                "(",
                "#item",
                ")"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "on-"
                  ]
                },
                "#...",
                "#item"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "data-"
                  ]
                },
                "#...",
                "on-",
                "#...",
                "#item"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "data-"
                  ]
                },
                "#...",
                "(",
                "#item",
                ")"
