              [
                {
                  "priority": "lowest",
                  "template": [
                    "bind-"
                  ]
                },
                "#...",
                "#item"
              ],
              [
                {
                  "priority": "lowest",
                  "template": [
                    "data-"
                  ]
                },
                "#...",
                "bind-",
                "#...",
                "#item"
